name: cecn-data-programming

docker_env:
    image: cecn-data-programming-image

entry_points:

    atsc:
        parameters:
            atsc_data: path
            n_epochs: {type: int, default: 1000}
            optimizer: {type: string, default: sgd}
            prec_init: {type: float, default: 0.7}
        command: "python -m label --atsc --n_epochs {n_epochs} --optimizer {optimizer} --prec_init {prec_init} {atsc_data}"

    values:
        parameters:
            values_data: path
            n_epochs: {type: int, default: 1000}
            optimizer: {type: string, default: sgd}
            prec_init: {type: float, default: 0.7}
        command: "python -m label --values --n_epochs {n_epochs} --optimizer {optimizer} --prec_init {prec_init} {values_data}"